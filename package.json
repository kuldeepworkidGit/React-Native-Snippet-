{
  "name": "react-native-snippet-by-kuldeep",
  "displayName": "React Native Snippet by Kuldeep",
  "description": "Useful React Native boilerplate shortcuts for fast development.",
  "publisher": "iKuldeepSingh",
  "version": "0.0.1",
  "type": "module",
  "engines": {
    "vscode": "^1.101.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "react-native-snippet-by-kuldeep.helloWorld",
        "title": "Hello World"
      }
    ],
    "snippets": [
    {
      "language": "javascript",
      "path": "./snippets/react-native-snippets.code-snippets"
    },
    {
      "language": "typescriptreact",
      "path": "./snippets/react-native-snippets.code-snippets"
    }
  ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "yarn run compile && yarn run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.101.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
